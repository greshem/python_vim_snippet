	===========
	
	"""
	import matplotlib.pyplot as plt
	import matplotlib.patches as patches
	
	# build a rectangle in axes coords
	left, width = .25, .5
	bottom, height = .25, .5
	right = left + width
	top = bottom + height
	
	fig = plt.figure()
	ax = fig.add_axes([0,0,1,1])
	
