	    t, ymin=0, ymax=1, where=s1 > 0, facecolor='green', alpha=0.5)
	ax.add_collection(collection)
	
	collection = collections.BrokenBarHCollection.span_where(
	    t, ymin=-1, ymax=0, where=s1 < 0, facecolor='red', alpha=0.5)
	ax.add_collection(collection)
	
	
	plt.show()
	


