	"""
	import matplotlib.pyplot as plt
	from numpy import arange
	from numpy.random import rand
	
	
	def gbar(ax, x, y, width=0.5, bottom=0):
	    X = [[.6, .6], [.7, .7]]
	    for left, top in zip(x, y):
	        right = left + width
	        ax.imshow(X, interpolation='bicubic', cmap=plt.cm.Blues,
